<h1> Home </h1>
<p> Life and school and school and life. </p>

<table>
	<thead>
		<tr>
			<th>Name</th>
			<th>Beginningyear</th>
			<th>Endingyear</th>
			<th colspan="3"></th>
		</tr>
	</thead>

	<tbody>
		<% @schools.each do |school| %>
			<tr>
				<td><%= school.name %></td>
				<td><%= school.beginningYear %></td>
				<td><%= school.endingYear %></td>
				<td><%= link_to 'Show', school %></td>
			</tr>
		<% end %>
	</tbody>
</table>

<%= link_to 'Life Events', life_events_path %>
<%= link_to 'Schools', schools_path %>
<%= link_to 'People', people_path %>
